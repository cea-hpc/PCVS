#!/bin/csh
# Routine to plot all test results for a given filesize.
#
if ($#argv != 3) then
	usage:
	echo "To plot all averaged results for a given test (ignore filesize)."
	echo "Usage: alldata file testname key"
	echo "   filesize in MB"
	echo "   key: "w","r","t","wI"...."
	exit 1
else
	awkallav -v testname=$argv[2]  -v key=$argv[3] $argv[1]
endif
#

