#
#  Makefile for MPI Test
#

MPITEST_LOCALLIB=@MPITEST_LIBPMPI@
MPITEST_LOCALOBJ=

sources:	${MPITEST_TESTNAME}.sh node.F

execs:		${MPITEST_TESTNAME}.sx node.${MPITEST_ARCH}${MPITEST_USERTAG}_bx

logs::		${MPITEST_TESTNAME}.${MPITEST_ARCH}_${MPITEST_NPROCS}${MPITEST_USERTAG}_L

results::	${MPITEST_TESTNAME}.${MPITEST_ARCH}_${MPITEST_NPROCS}${MPITEST_USERTAG}_R

node.$(MPITEST_ARCH)${MPITEST_USERTAG}_bx: $(MPITEST_CONFIG_FILE) $(MPITEST_LOCALOBJ)
