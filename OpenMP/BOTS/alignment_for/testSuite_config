#!/bin/sh
REQUIRE_OPENMP=true


set_openmp_threads_list

MPC_PROC_NB_LIST=1
MPC_TASK_NB_LIST=1
MPC_THREAD_NB_LIST=1

EXPECTED_EXIT_CODE=0

COMMON_FILES="${PCVS_CSOURCE_DIR}/../common/bots_main.c ${PCVS_CSOURCE_DIR}/../common/bots_common.c ${PCVS_CSOURCE_DIR}/sequence.c"

COMMAND_OPTIONS="-c -f ${PCVS_CSOURCE_DIR}/../inputs/alignment/prot.20.aa"

CFLAGS="${CFLAGS} -O3 -fopenmp ${PRIVATIZATION} -I${PCVS_CSOURCE_DIR}/../common -I${PCVS_CSOURCE_DIR} -lm"

openmp_configuration "for20" "alignment.c"
