#!/bin/sh
REQUIRE_OPENMP=true


set_openmp_threads_list

MPC_AUTO_KILL_TIMEOUT="900"
EXPECTED_EXIT_CODE=0

COMMON_FILES="${MPC_TEST_CURRENT_SOURCE_DIR}/../common/bots_main.c ${MPC_TEST_CURRENT_SOURCE_DIR}/../common/bots_common.c"

COMMAND_OPTIONS="-c -n 14"

CFLAGS="${CFLAGS} -O3 -fopenmp ${PRIVATIZATION} -I${MPC_TEST_CURRENT_SOURCE_DIR}/../common -I${MPC_TEST_CURRENT_SOURCE_DIR} -lm"

openmp_configuration "board14"
