#!/bin/sh
REQUIRE_OPENMP=true


set_openmp_threads_list

MPC_PROC_NB_LIST=1
MPC_TASK_NB_LIST=1
MPC_THREAD_NB_LIST=1

EXPECTED_EXIT_CODE=0

COMMON_FILES="${MPC_TEST_CURRENT_SOURCE_DIR}/../common/bots_main.c ${MPC_TEST_CURRENT_SOURCE_DIR}/../common/bots_common.c ${MPC_TEST_CURRENT_SOURCE_DIR}/brg_sha1.c"

COMMAND_OPTIONS="-c -f ${MPC_TEST_CURRENT_SOURCE_DIR}/../inputs/uts/tiny.input"

CFLAGS="${CFLAGS} -O3 -fopenmp ${PRIVATIZATION} -I${MPC_TEST_CURRENT_SOURCE_DIR}/../common -I${MPC_TEST_CURRENT_SOURCE_DIR} -lm"

openmp_configuration "tiny" "uts.c"
