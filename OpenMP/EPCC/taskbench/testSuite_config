#!/bin/sh

REQUIRE_OPENMP=true
COMMON_FILES="${MPC_TEST_CURRENT_SOURCE_DIR}/../common/common.c"
CFLAGS="${CFLAGS} -fopenmp ${PRIVATIZATION} -DOMPVER2 -DOMPVER3 -I${MPC_TEST_CURRENT_SOURCE_DIR}/../common/ -lm"


set_openmp_threads_list

MPC_PROC_NB_LIST=1
MPC_TASK_NB_LIST=1
MPC_THREAD_NB_LIST=1

openmp_configuration
