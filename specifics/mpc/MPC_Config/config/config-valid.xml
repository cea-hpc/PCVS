<?xml version='1.0'?>
<mpc xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="mpc-config.xsd">
	<profiles>

		<profile>
			<name>default</name>
			<modules>
				<accelerator>
					<enabled>false</enabled>
					<cuda>
						<enabled>false</enabled>
					</cuda>
					<openacc>
						<enabled>false</enabled>
					</openacc>
					<opencl>
						<enabled>false</enabled>
					</opencl>
				</accelerator>
				<allocator>
					<numa_migration>false</numa_migration>
					<realloc_factor>2</realloc_factor>
					<realloc_threashold>50 MB</realloc_threashold>
					<numa>true</numa>
					<strict>false</strict>
					<keep_mem>500 MB</keep_mem>
					<keep_max>8 MB</keep_max>
				</allocator>
				<launcher>
					<verbosity>0</verbosity>
					<banner>true</banner>
					<autokill>0</autokill>
					<user_launchers>default</user_launchers>
					<keep_rand_addr>true</keep_rand_addr>
					<disable_rand_addr>false</disable_rand_addr>
					<disable_mpc>false</disable_mpc>
					<thread_init>sctk_use_ethread_mxn</thread_init>
					<nb_task>1</nb_task>
					<nb_process>1</nb_process>
					<nb_processor>0</nb_processor>
					<nb_node>1</nb_node>
					<launcher>none</launcher>
					<max_try>10</max_try>
					<vers_details>false</vers_details>
					<profiling>stdout</profiling>
					<enable_smt>false</enable_smt>
					<share_node>false</share_node>
					<restart>false</restart>
					<checkpoint>false</checkpoint>
					<migration>false</migration>
					<report>false</report>
				</launcher>
				<debugger>
					<colors>true</colors>
					<max_filename_size>1024</max_filename_size>
				</debugger>
				<collectives_intra>
					<barrier_intra>__INTERNAL__PMPI_Barrier_intra</barrier_intra>
					<bcast_intra>__INTERNAL__PMPI_Bcast_intra</bcast_intra>
					<allgather_intra>__INTERNAL__PMPI_Allgather_intra</allgather_intra>
					<allgatherv_intra>__INTERNAL__PMPI_Allgatherv_intra</allgatherv_intra>
					<alltoall_intra>__INTERNAL__PMPI_Alltoall_intra</alltoall_intra>
					<alltoallv_intra>__INTERNAL__PMPI_Alltoallv_intra</alltoallv_intra>
					<alltoallw_intra>__INTERNAL__PMPI_Alltoallw_intra</alltoallw_intra>
					<gather_intra>__INTERNAL__PMPI_Gather_intra</gather_intra>
					<gatherv_intra>__INTERNAL__PMPI_Gatherv_intra</gatherv_intra>
					<scatter_intra>__INTERNAL__PMPI_Scatter_intra</scatter_intra>
					<scatterv_intra>__INTERNAL__PMPI_Scatterv_intra</scatterv_intra>
					<scan_intra>__INTERNAL__PMPI_Scan_intra</scan_intra>
					<exscan_intra>__INTERNAL__PMPI_Exscan_intra</exscan_intra>
					<reduce_intra>__INTERNAL__PMPI_Reduce_intra</reduce_intra>
					<allreduce_intra>__INTERNAL__PMPI_Allreduce_intra</allreduce_intra>
					<reduce_scatter_intra>__INTERNAL__PMPI_Reduce_scatter_intra</reduce_scatter_intra>
					<reduce_scatter_block_intra>__INTERNAL__PMPI_Reduce_scatter_block_intra</reduce_scatter_block_intra>
				</collectives_intra>
				<collectives_inter>
					<barrier_inter>__INTERNAL__PMPI_Barrier_inter</barrier_inter>
					<bcast_inter>__INTERNAL__PMPI_Bcast_inter</bcast_inter>
					<allgather_inter>__INTERNAL__PMPI_Allgather_inter</allgather_inter>
					<allgatherv_inter>__INTERNAL__PMPI_Allgatherv_inter</allgatherv_inter>
					<alltoall_inter>__INTERNAL__PMPI_Alltoall_inter</alltoall_inter>
					<alltoallv_inter>__INTERNAL__PMPI_Alltoallv_inter</alltoallv_inter>
					<alltoallw_inter>__INTERNAL__PMPI_Alltoallw_inter</alltoallw_inter>
					<gather_inter>__INTERNAL__PMPI_Gather_inter</gather_inter>
					<gatherv_inter>__INTERNAL__PMPI_Gatherv_inter</gatherv_inter>
					<scatter_inter>__INTERNAL__PMPI_Scatter_inter</scatter_inter>
					<scatterv_inter>__INTERNAL__PMPI_Scatterv_inter</scatterv_inter>
					<reduce_inter>__INTERNAL__PMPI_Reduce_inter</reduce_inter>
					<allreduce_inter>__INTERNAL__PMPI_Allreduce_inter</allreduce_inter>
					<reduce_scatter_inter>__INTERNAL__PMPI_Reduce_scatter_inter</reduce_scatter_inter>
					<reduce_scatter_block_inter>__INTERNAL__PMPI_Reduce_scatter_block_inter</reduce_scatter_block_inter>
				</collectives_inter>
				<progress_thread>
					<use_progress_thread>1</use_progress_thread>
					<progress_thread_binding>sctk_get_progress_thread_binding_bind</progress_thread_binding>
				</progress_thread>
				<mpc>
					<log_debug>false</log_debug>
					<hard_checking>false</hard_checking>
					<buffering>false</buffering>
				</mpc>
				<inter_thread_comm>
					<barrier_arity>8</barrier_arity>
					<broadcast_arity_max>32</broadcast_arity_max>
					<broadcast_max_size>1024</broadcast_max_size>
					<broadcast_check_threshold>512</broadcast_check_threshold>
					<allreduce_arity_max>8</allreduce_arity_max>
					<allreduce_max_size>4096</allreduce_max_size>
					<allreduce_check_threshold>8192</allreduce_check_threshold>
					<ALLREDUCE_MAX_SLOT>65536</ALLREDUCE_MAX_SLOT>
					<collectives_init_hook>sctk_collectives_init_opt_noalloc_split_messages</collectives_init_hook>
				</inter_thread_comm>
				<low_level_comm>
					<checksum>true</checksum>
					<send_msg>sctk_network_send_message_default</send_msg>
					<network_mode>default</network_mode>
					<dyn_reordering>false</dyn_reordering>
					<enable_idle_polling>false</enable_idle_polling>
					<ib_global>
						<mmu_cache_enabled>1</mmu_cache_enabled>
						<mmu_cache_entry_count>1000</mmu_cache_entry_count>
						<mmu_cache_maximum_size>4 GB</mmu_cache_maximum_size>
						<mmu_cache_maximum_pin_size>1 GB</mmu_cache_maximum_pin_size>
					</ib_global>
				</low_level_comm>
				<openmp>
					<vp>0</vp>
					<schedule>static</schedule>
					<nb_threads>1</nb_threads>
					<adjustment>false</adjustment>
					<proc_bind>true</proc_bind>
					<nested>false</nested>
					<stack_size>0</stack_size>
					<wait_policy>0</wait_policy>
					<thread_limit>0</thread_limit>
					<max_active_levels>0</max_active_levels>
					<tree></tree>
					<max_threads>64</max_threads>
					<max_alive_for_dyn>7</max_alive_for_dyn>
					<max_alive_for_guided>3</max_alive_for_guided>
					<max_alive_sections>3</max_alive_sections>
					<max_alive_single>3</max_alive_single>
					<warn_nested>false</warn_nested>
					<mode>simple-mixed</mode>
					<affinity>balanced</affinity>
				</openmp>
				<profiler>
					<file_prefix>mpc_profile</file_prefix>
					<append_date>true</append_date>
					<color_stdout>true</color_stdout>
					<level_colors>
						<level>#3A4D85</level>
						<level>#82A2FF</level>
						<level>#B8BDCB</level>
						<level>#5D6782</level>
						<level>#838383</level>
						<level>#5A5757</level>
					</level_colors>
				</profiler>
				<thread>
					<spin_delay>10</spin_delay>
					<interval>10</interval>
					<kthread_stack_size>10 MB</kthread_stack_size>
					<placement_policy>sctk_get_init_vp_and_nbvp_default</placement_policy>
				</thread>
				<scheduler>
					<timestamp_threshold>0</timestamp_threshold>
					<task_polling_thread_basic_priority>20</task_polling_thread_basic_priority>
					<task_polling_thread_basic_priority_step>20</task_polling_thread_basic_priority_step>
					<task_polling_thread_current_priority_step>20</task_polling_thread_current_priority_step>
					<sched_NBC_Pthread_basic_priority>20</sched_NBC_Pthread_basic_priority>
					<sched_NBC_Pthread_basic_priority_step>20</sched_NBC_Pthread_basic_priority_step>
					<sched_NBC_Pthread_current_priority_step>20</sched_NBC_Pthread_current_priority_step>
					<mpi_basic_priority>20</mpi_basic_priority>
					<omp_basic_priority>20</omp_basic_priority>
					<posix_basic_priority>20</posix_basic_priority>
					<progress_basic_priority>20</progress_basic_priority>
				</scheduler>
			</modules>
			<networks>

			</networks>
		</profile>
		<profile>
			<name>test</name>
			<modules>
				<allocator>
					<numa>false</numa>
					<keep_mem>5 MB</keep_mem>
					<keep_max>120 PB</keep_max>
				</allocator>
                <launcher>
                    <verbosity>0</verbosity>
                    <nb_task>8</nb_task>
                    <thread_init>sctk_use_pthread</thread_init>
                </launcher>
			</modules>
		</profile>
		<profile>
			<name>debug</name>
			<modules>
				<launcher>
					<verbosity>2</verbosity>
					<banner>true</banner>
					<nb_task>4</nb_task>
					<thread_init>sctk_use_ethread</thread_init>
				</launcher>
			</modules>
		</profile>
	</profiles>
	<mappings>
		<mapping>
			<selectors>
				<env name="MPC_DEBUG">true</env>
				<env name="MPC_GDB">gdb</env>
			</selectors>
			<profiles>
				<profile>debug</profile>
			</profiles>
		</mapping>
                <mapping>
                        <selectors>
                                <env name="MPC_TEST">true</env>
                        </selectors>
                        <profiles>
                                <profile>test</profile>
                        </profiles>
                </mapping>
	</mappings>
</mpc>

