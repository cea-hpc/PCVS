#!/bin/sh
REQUIRE_OPENMP=true


set_openmp_threads_list

MPC_AUTO_KILL_TIMEOUT="900"
EXPECTED_EXIT_CODE=0

COMMON_FILES="${PCVS_CSOURCE_DIR}/../common/bots_main.c ${PCVS_CSOURCE_DIR}/../common/bots_common.c"

COMMAND_OPTIONS="-c -n 14"

CFLAGS="${CFLAGS} -O3 -fopenmp ${PRIVATIZATION} -I${PCVS_CSOURCE_DIR}/../common -I${PCVS_CSOURCE_DIR} -lm"

openmp_configuration "board14"
