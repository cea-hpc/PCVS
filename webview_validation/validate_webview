#!/bin/sh

tar -xzf `dirname $0`/origin_results.tar.gz
mv last_results last_results_orig

tar -xzf $1
mv last_results last_results_new

gcc -g `xml2-config --cflags --libs` -o xml_webview_validation `dirname $0`/xml_webview_validation.c

./xml_webview_validation last_results_orig/webview/generated/main.xml last_results_new/webview/generated/main.xml
